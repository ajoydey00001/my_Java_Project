<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="700.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Client.BuyPlayerListController">
   <children>
      <TableView fx:id="tableView" layoutX="8.0" prefHeight="472.0" prefWidth="883.0" />
      <Label layoutX="14.0" layoutY="472.0" text="Please Enter A Player Name">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="reset" layoutX="235.0" layoutY="634.0" mnemonicParsing="false" onAction="#resetAction" text="Reset">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <TextField fx:id="name" layoutX="14.0" layoutY="507.0" prefHeight="26.0" prefWidth="883.0" />
      <Label layoutX="14.0" layoutY="538.0" text="Please Enter An Amount Of Money">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TextField fx:id="money" layoutX="14.0" layoutY="574.0" prefHeight="26.0" prefWidth="883.0" />
      <Button fx:id="enter" layoutX="450.0" layoutY="634.0" mnemonicParsing="false" onAction="#enterAction" text="Enter">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
