<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Client.SellingController">
   <children>
      <Label layoutX="92.0" layoutY="58.0" text="Please Enter A Name Of A Player">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TextField fx:id="name" layoutX="92.0" layoutY="108.0" prefHeight="35.0" prefWidth="503.0" />
      <Button fx:id="reset" layoutX="190.0" layoutY="320.0" mnemonicParsing="false" onAction="#resetAction" text="Reset">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Label layoutX="92.0" layoutY="151.0" text="Please Set An Amount Of Money">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TextField fx:id="money" layoutX="92.0" layoutY="200.0" prefHeight="35.0" prefWidth="503.0" />
      <Button fx:id="enter" layoutX="363.0" layoutY="320.0" mnemonicParsing="false" onAction="#enterAction" text="Enter">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
